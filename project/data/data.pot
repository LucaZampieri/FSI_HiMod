[./set]
  verbose = 1
[../]

[himod]
  drho    = 0
  mx      = 3 # modes for Vx
  mr      = 3 # modes for Vr
  mtheta  = 3 # modes for Vtheta
  mp      = 3 # modes for the pressure
[../]

[mesh]
    num_elements        = 30 # num of elements in x-discretization
    Poly_type_velocity  = P2
    Poly_type_pressure  = P1
[../]

[fluid]
    [./physics]
        mu        = 0.0279
        nu        = 0.0222
        rho_f     = 1050
        density   = 1.
        Rin       = 0.5
        Rout      = 0.5
        occlusion = 0.45
        deltaP    = 5.
        z0        = 1.335
        delta0    = 0.45
    [../]
    [./structure]
        L         = 3         # length of the cylinder
        rho_s     = 1         # Density (specific mass) of the membrane (Wall)
        h_s       = 0.05      # Width of the membrane (Wall)
        e         = 5.e5      # e = Young * width / ((1 - poisson*poisson) * R*R)
    [../]
[../]

[time]
    t0  = 0            # initial time
    dt  = 0.25         # time step
    T   = 1            # final time
[../]

[fixedPoint]
    PF_max  = 1000
    tol    = 1e-4
[../]

[prec]
    prectype                     = Ifpack # Ifpack or ML
    displayList                  = false

    [./ifpack]
        overlap                  = 2

        [./fact]
            ilut_level-of-fill   = 1
            drop_tolerance       = 1.e-5
            relax_value          = 0

        [../amesos]
            solvertype           = Amesos_KLU # Amesos_KLU or Amesos_Umfpack

        [../partitioner]
            overlap              = 2

        [../schwarz]
            reordering_type      = none #metis, rcm, none
            filter_singletons    = true

    [../]
  [../]
[../]
[exporter]
    start=0
    save=1
    multimesh=false
[../]

[functions]
    ux0 = 1
    ur0 = 0
    utheta0 = 0

    Radius  = 1
    dRadius = 0

    p1 = 14000
    p2 = 13000

    fx = 0
    fr = 0
    ftheta = 0
[../]
