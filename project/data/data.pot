[./set]
  verbose = 1
[../]

[himod]
  drho    = 0
  mx      = 5                # modes for Vx
  mr      = 5                # modes for Vr
  mtheta  = 5                # modes for Vtheta
  mp      = 3                # modes for the pressure
[../]

[mesh]
    num_elements        = 15 # num of elements in x-discretization
    Poly_type_velocity  = P2
    Poly_type_pressure  = P1
[../]

[fluid]
    [./physics]
        nu        = 0.035     # cinematic viscosity
    [../]
    [./structure]
        L         = 500         # length of the cylinder
        rho_s     = 1         # Density (specific mass) of the membrane (Wall)
        h_s       = 0.01      # Width of the membrane (Wall)
        e         = 4.e5      # e = Young * width / ((1 - poisson*poisson) * R*R)
    [../]
[../]

[time]
    t0  = 0.0           # initial time
    dt  = 0.05           # time step
    T   = 2            # final time
[../]

[functions]
    ux0 = 30000000
    ur0 = 0 #10     #100*sin(t*100*2*3.14)
    utheta0 = 0

    Radius  = 1 # Radius function
    dRadius = 0

    p1 = 5000 #*t
    p2 = 0

    fx     = 0                           #1000*sin(t)
    fr     = 0       #50000*1*x    #2000*t
    ftheta = 0                           #1000*cos(t)
[../]

######  Data for the solvers #####

[fixedPoint]
    PF_max  = 1000
    tol     = 1e-4
[../]

[prec]  # used by the solver, see FSISolver.cpp
    prectype                     = Ifpack # Ifpack or ML
    displayList                  = false

    [./ifpack]
        overlap                  = 2

        [./fact]
            ilut_level-of-fill   = 1
            drop_tolerance       = 1.e-5
            relax_value          = 0

        [../amesos]
            solvertype           = Amesos_KLU # Amesos_KLU or Amesos_Umfpack

        [../partitioner]
            overlap              = 2

        [../schwarz]
            reordering_type      = none #metis, rcm, none
            filter_singletons    = true

    [../]
  [../]
[../]
[exporter]
    start=0
    save=1
    multimesh=false
[../]
